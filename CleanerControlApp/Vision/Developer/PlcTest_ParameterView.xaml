<UserControl x:Class="CleanerControlApp.Vision.Developer.PlcTest_ParameterView"
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
 mc:Ignorable="d">
 <Grid Background="White">
 <Grid.RowDefinitions>
 <RowDefinition Height="Auto" />
 <RowDefinition Height="10" />
 <RowDefinition Height="*" />
 </Grid.RowDefinitions>

 <!-- Top buttons -->
 <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Grid.Row="0" Margin="0,0,0,10">
 <Button x:Name="BtnReadParams" Content="Read Params" Width="120" Height="36" Margin="0,0,10,0" Click="BtnReadParams_Click" />
 <Button x:Name="BtnWriteParams" Content="Write Params" Width="120" Height="36" Margin="0,0,10,0" Click="BtnWriteParams_Click" />
 <!-- Added preview mouse/touch handlers so press sets command and release clears it -->
 <Button x:Name="BtnSet" Content="Set" Width="120" Height="36" Click="BtnSet_Click"
 PreviewMouseLeftButtonDown="BtnSet_PreviewMouseLeftButtonDown"
 PreviewMouseLeftButtonUp="BtnSet_PreviewMouseLeftButtonUp"
 TouchDown="BtnSet_TouchDown"
 TouchUp="BtnSet_TouchUp" />
 </StackPanel>

 <ScrollViewer Grid.Row="2" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
 <!-- change to4 columns x1 row -->
 <UniformGrid Columns="4" Rows="1" Margin="6" HorizontalAlignment="Left">

 <!-- Axis1 -->

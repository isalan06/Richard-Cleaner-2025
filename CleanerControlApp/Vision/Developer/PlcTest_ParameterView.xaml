 <TextBox x:Name="Axis1_JogSpeedH_Input" Grid.Row="0" Grid.Column="2" Width="80" HorizontalContentAlignment="Right" PreviewTextInput="NumericOnly_PreviewTextInput" DataObject.Pasting="NumericOnly_Pasting" LostFocus="ParamInput_LostFocus" Tag="Param_Write_Axis1JogSpeedH" />


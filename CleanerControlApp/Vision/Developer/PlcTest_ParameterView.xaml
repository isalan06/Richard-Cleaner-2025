 <TextBox x:Name="Axis1_HomeSpeedM_Input" Grid.Row="4" Grid.Column="2" Width="80" HorizontalContentAlignment="Right" PreviewTextInput="NumericOnly_PreviewTextInput" DataObject.Pasting="NumericOnly_Pasting" LostFocus="ParamInput_LostFocus" Tag="Param_Write_Axis1HomeSpeedM" />

